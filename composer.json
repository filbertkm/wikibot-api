{
  "name": "wikibot/api",
  "type": "library",
  "description": "MediaWiki api client library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/filbertkm/wikibase-store.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/filbertkm/WikiClient.git"
    }
  ],
  "require": {
    "data-values/serialization": "~1.0",
    "wikibase/data-model": "~2.0",
    "wikibase/data-model-serialization": "~1.2",
    "wikibase/store": "dev-master",
    "wikiclient/wikiclient": "dev-master"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "3.7.*"
  },
  "autoload": {
    "psr-4": {
      "Wikibot\\Api\\": "src"
    }
  }
}
